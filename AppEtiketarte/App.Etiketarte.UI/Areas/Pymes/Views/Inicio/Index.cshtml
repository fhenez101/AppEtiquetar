<div class="container">
    <header>
        <h1 data-animated="GoIn"><b>COUNTER</b> site is coming soon...</h1>
    </header>
    <div id="timer" data-animated="FadeIn">
        <p id="message"></p>
        <div id="days" class="timer_box"></div>
        <div id="hours" class="timer_box"></div>
        <div id="minutes" class="timer_box"></div>
        <div id="seconds" class="timer_box"></div>
    </div>
    <div class="col-lg-4 col-lg-offset-4 mt centered">
        <h4>LET ME KNOW WHEN YOU LAUNCH</h4>
        <form class="form-inline" role="form">
            <div class="form-group">
                <label class="sr-only" for="exampleInputEmail2">Email address</label>
                <input type="email" class="form-control" id="exampleInputEmail2" placeholder="Enter email">
            </div>
            <button type="submit" class="btn btn-info">Submit</button>
        </form>
    </div>
</div>
<div id="layer"></div>
<div id="slider" class="rev_slider">
    <ul>
        <li data-transition="slideleft" data-slotamount="1" data-thumb="@Url.Content("~/Content/Image/Pymes/1.jpg")">
            <img src="@Url.Content("~/Content/Image/Pymes/1.jpg")">
        </li>
        <li data-transition="slideleft" data-slotamount="1" data-thumb="@Url.Content("~/Content/Image/Pymes/2.jpg")">
            <img src="@Url.Content("~/Content/Image/Pymes/2.jpg")">
        </li>
        <li data-transition="slideleft" data-slotamount="1" data-thumb="@Url.Content("~/Content/Image/Pymes/3.jpg")">
            <img src="@Url.Content("~/Content/Image/Pymes/3.jpg")">
        </li>
        <li data-transition="slideleft" data-slotamount="1" data-thumb="@Url.Content("~/Content/Image/Pymes/4.jpg")">
            <img src="@Url.Content("~/Content/Image/Pymes/4.jpg")">
        </li>
    </ul>
</div>